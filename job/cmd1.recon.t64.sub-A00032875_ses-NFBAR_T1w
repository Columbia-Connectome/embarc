    recon-all -s _ses-NFBAR -i /lus/theta-fs0/projects/AD_Brain_Imaging/anal/NKI/data/sub-A00032875/ses-NFBAR/anat/sub-A00032875_ses-NFBAR_T1w.nii.gz -parallel -openmp 64
