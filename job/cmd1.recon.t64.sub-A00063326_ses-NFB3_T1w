    SUBJECTS_DIR=/local/scratch/
    ITK_GLOBAL_DEFAULT_NUMBER_OF_THREADS=64
    recon-all -s _ses-NFB3 -i /lus/theta-fs0/projects/AD_Brain_Imaging/anal/NKI/data/sub-A00063326/ses-NFB3/anat/sub-A00063326_ses-NFB3_T1w.nii.gz -parallel -openmp 64
